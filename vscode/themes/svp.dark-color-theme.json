{
	"name": "svp.dark",
	"type": "dark",
	"colors": {
		"editor.background": "#0b0f12", // <- bg
		"editor.foreground": "#ecf2f8",
		"editor.selectionBackground": "#21262d",
		"editorGroupHeader.tabsBackground": "#0b0f12",
		"editorLineNumber.foreground": "#404B55",
		"editorWhitespace.foreground": "#2A303D",
		"editorWidget.background": "#0b0f12",
		"editorWidget.border": "#404B55",
		"focusBorder": "#161b22",
		"list.activeSelectionBackground": "#21262d",
		"list.errorForeground": "#fa7970",
		"list.hoverBackground": "#21262d",
		"list.inactiveSelectionBackground": "#21262d",
		"list.warningForeground": "#faa356",
		"panel.border": "#161b22",
		"sideBar.background": "#0b0f12",
		"sideBar.border": "#161b22",
		"sideBarTitle.foreground": "#ecf2f8",
		"statusBar.background": "#0b0f12",
		"statusBar.foreground": "#404B55",
		"sideBarSectionHeader.background": "#0b0f12",
		"sideBarSectionHeader.foreground": "#404B55",
		"tab.activeBackground": "#0b0f12",
		"tab.inactiveBackground": "#0b0f12",
		"titleBar.activeBackground": "#0b0f12",
		"titleBar.border": "#0b0f12",
		"titleBar.inactiveBackground": "#0b0f12",
		"peekView.border": "#161b22",
		"peekViewTitle.background": "#161b22",
		"panelTitle.activeForeground": "#404B55",
		"breadcrumb.foreground": "#404B55",
		"breadcrumb.focusForeground": "#404B55",
		"commandCenter.background": "#0b0f12",
		"commandCenter.foreground": "#404B55",
		"commandCenter.activeForeground": "#404B55",
		"commandCenter.inactiveForeground": "#404B55",
		"commandCenter.border": "#404B55",
		"icon.foreground": "#404B55",
		"input.background": "#0b0f12",
		"titleBar.activeForeground": "#404B55",
	},
	"tokenColors": [
		{
			"scope": [
				"keyword",
				"storage.type.tsx",
				"storage.type.function.tsx",
				"storage.type.interface.tsx",
			],
			"settings": {
				"foreground": "#cc7833"
			}
		},
		{
			"scope": [
				"entity.name.type",
				"storage.type",
				"support.type.graphql",
				"meta.object-literal.key.tsx",
				"variable.other.property.go",
			],
			"settings": {
				"foreground": "#d6d6d6"
			}
		},
		{
			"scope": [
				"entity.name.function",
				"support.function",
				"entity.name.tag",
			],
			"settings": {
				"foreground": "#ffc66d"
			}
		},
		{
			"scope": [
				"variable",
			],
			"settings": {
				"foreground": "#FFFFFF"
			}
		},
		{
			"scope": [
				"constant"
			],
			"settings": {
				"foreground": "#ecf2f8"
			}
		},
		{
			"scope": [
				"string",
				"constant.numeric",
			],
			"settings": {
				"foreground": "#b4c973"
			}
		},
		{
			"scope": [
				"punctuation.definition.string",
			],
			"settings": {
				"foreground": "#aaba78"
			}
		},
		{
			"scope": [
				"support.type",
				"storage.type.numeric.go",
			],
			"settings": {
				"foreground": "#ff9834"
			}
		},
		{
			"scope": [
				"punctuation.definition.begin.bracket",
				"punctuation.definition.end.bracket",
				"punctuation.definition.tag.begin",
				"punctuation.definition.tag.end",
				"punctuation.section.property-list.begin.bracket",
				"punctuation.section.property-list.end.bracket",
				"punctuation.operation.graphql",
				"meta.block.tsx",
			],
			"settings": {
				"foreground": "#89929b"
			}
		},
		{
			"scope": [
				"punctuation.other.period",
				"punctuation.other.comma",
			],
			"settings": {
				"foreground": "#ADB5BD"
			}
		},
		{
			"scope": [
				"comment"
			],
			"settings": {
				"foreground": "#797979",
				"fontStyle": "italic",
			}
		},
	]
}
