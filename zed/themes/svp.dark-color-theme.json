{
  "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
  "name": "svp.dark",
  "author": "Sergei Parshukov",
  "themes": [
    {
      "name": "svp.dark",
      "appearance": "dark",
      "style": {
        "border": "#161b22",
        "border.variant": "#161b22",
        "border.focused": "#161b22",
        "border.selected": "#161b22",
        "border.transparent": "#161b22",
        "border.disabled": "#161b22",
        "elevated_surface.background": "#191918",
        "background": "#0b0f12",
        "element.hover": "#21262d",
        "element.selected": "#21262d",
        "ghost_element.hover": "#21262d",
        "ghost_element.selected": "#21262d",
        "text": "#ffffff",
        "text.accent": "#ffc66d",
        "status_bar.background": "#0b0f12",
        "title_bar.background": "#0b0f12",
        "toolbar.background": "#0b0f12",
        "tab_bar.background": "#0b0f12",
        "tab.inactive_background": "#0b0f12",
        "tab.active_background": "#0b0f12",
        "search.match_background": "#515c6a",
        "scrollbar.track.background": "#0b0f12",
        "editor.foreground": "#ecf2f8",
        "editor.background": "#0b0f12",
        "editor.gutter.background": "#0b0f12",
        "editor.line_number": "#404B55",
        "editor.active_line_number": "#ecf2f8",
        "editor.wrap_guide": "#161b22",
        "editor.active_wrap_guide": "#161b22",
        "editor.document_highlight.bracket_background": "#727a84",
        "terminal.background": "#0b0f12",
        "terminal.ansi.yellow": "#ffc66d",
        "terminal.ansi.bright_yellow": "#ffc66d",
        "terminal.ansi.dim_yellow": "#ffc66d",
        "error": "#cc4242",
        "error.background": "#cc4242",
        "error.border": "#cc4242",
        "hint": "#969696",
        "modified": "#ffc66d",
        "warning": "#ffc66d",
        "warning.background": "#cc7833",
        "warning.border": "#cc7833",
        "players": [
          {
            "selection": "#3248543d"
          }
        ],
        "syntax": {
          "attribute": {
            "color": "#d6d6d6"
          },
          "comment": {
            "color": "#797979ff",
            "font_style": "italic"
          },
          "comment.doc": {
            "color": "#797979ff",
            "font_style": "italic"
          },
          "constant": {
            "color": "#ecf2f8"
          },
          "constructor": {
            "color": "#ffc66d"
          },
          "function": {
            "color": "#ffc66d"
          },
          "keyword": {
            "color": "#cc7833"
          },
          "namespace": {
            "color": "#d6d6d6"
          },
          "number": {
            "color": "#b4c973"
          },
          "operator": {
            "color": "#cc7833"
          },
          "punctuation": {
            "color": "#89929b"
          },
          "punctuation.bracket": {
            "color": "#89929b"
          },
          "punctuation.special": {
            "color": "#89929b"
          },
          "punctuation.delimiter": {
            "color": "#89929b"
          },
          "punctuation.list_marker": {
            "color": "#89929b"
          },
          "string": {
            "color": "#b4c973"
          },
          "string.escape": {
            "color": "#b4c973"
          },
          "string.regex": {
            "color": "#b4c973"
          },
          "string.special": {
            "color": "#b4c973"
          },
          "string.special.symbol": {
            "color": "#b4c973"
          },
          "tag": {
            "color": "#ffc66d"
          },
          "text.literal": {
            "color": "#b4c973"
          },
          "type": {
            "color": "#ffe2b3"
          },
          "variable": {
            "color": "#ffffff"
          },
          "variable.special": {
            "color": "#ff00ff"
          }
        }
      }
    }
  ]
}
